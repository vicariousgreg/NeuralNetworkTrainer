<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<HBox prefHeight="644.0" prefWidth="630.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.InteractController">
   <children>
      <VBox alignment="CENTER" layoutX="-1.0" layoutY="2.0" prefHeight="644.0" prefWidth="630.0" spacing="5.0">
         <children>
            <HBox>
               <children>
                  <VBox spacing="5.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#correct" prefHeight="25.0" prefWidth="242.0" text="Correct!" textAlignment="CENTER" />
                        <ScrollPane prefHeight="430.0" prefWidth="246.0">
                           <content>
                              <ListView fx:id="classificationList" prefHeight="426.0" prefWidth="237.0" />
                           </content>
                        </ScrollPane>
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" prefHeight="458.0" prefWidth="381.0" spacing="5.0">
                     <children>
                        <ColorPicker fx:id="colorPicker" onAction="#setColor" prefHeight="25.0" prefWidth="390.0" />
                        <Rectangle fx:id="colorBox" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="428.0" stroke="BLACK" strokeType="INSIDE" width="379.0" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <VBox alignment="CENTER" prefHeight="167.0" prefWidth="624.0" spacing="2.0">
               <children>
                  <HBox spacing="300.0">
                     <children>
                        <Label prefHeight="25.0" prefWidth="141.0" text="Short Term Memory:" />
                        <Button fx:id="memoryButton" mnemonicParsing="false" onAction="#viewMemory" prefHeight="25.0" prefWidth="178.0" text="View Memory" textAlignment="CENTER" />
                     </children>
                     <VBox.margin>
                        <Insets left="5.0" />
                     </VBox.margin>
                  </HBox>
                  <ScrollPane prefHeight="124.0" prefViewportHeight="159.0" prefViewportWidth="624.0" prefWidth="630.0">
                     <content>
                        <FlowPane fx:id="shortTermMemoryBox" prefHeight="117.0" prefWidth="605.0" />
                     </content>
                  </ScrollPane>
               </children>
               <VBox.margin>
                  <Insets top="2.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </children>
</HBox>
