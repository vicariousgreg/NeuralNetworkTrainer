<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<Pane prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.MainController">
   <children>
      <HBox prefHeight="30.0" prefWidth="1200.0">
         <children>
            <MenuBar prefHeight="30.0" prefWidth="1000.0">
              <menus>
                  <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#newNetwork" text="New Network" />
                        <MenuItem mnemonicParsing="false" onAction="#loadNetwork" text="Load Network" />
                        <MenuItem mnemonicParsing="false" onAction="#saveNetwork" text="Save Network" />
                        <MenuItem mnemonicParsing="false" onAction="#closeNetwork" text="Close Network" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Network">
                      <items>
                        <MenuItem mnemonicParsing="false" onAction="#editParameters" text="Edit Parameters" />
                      </items>
                  </Menu>
              </menus>
            </MenuBar>
             <ProgressBar fx:id="progressBar" layoutX="400.0" prefHeight="30.0" prefWidth="200.0" progress="0.0" />
         </children>
      </HBox>
      <BorderPane layoutX="4.0" layoutY="29.0" prefHeight="558.0" prefWidth="1200.0">
         <left>
            <fx:include fx:id="interact" source="interact.fxml" />
         </left>
         <right>
            <fx:include fx:id="examine" source="examine.fxml" />
         </right>
      </BorderPane>
   </children>
</Pane>
