<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controller.MainController">
   <children>
      <HBox>
         <children>
            <MenuBar prefHeight="30.0" prefWidth="402.0">
              <menus>
                  <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#newNetwork" text="New Network" />
                        <MenuItem mnemonicParsing="false" onAction="#loadNetwork" text="Load Network" />
                        <MenuItem mnemonicParsing="false" onAction="#saveNetwork" text="Save Network" />
                        <MenuItem mnemonicParsing="false" onAction="#closeNetwork" text="Close Network" />
                    </items>
                  </Menu>
                  <Menu mnemonicParsing="false" text="Network">
                      <items>
                          <MenuItem mnemonicParsing="false" onAction="#consolidateMemory" text="Consolidate Memory" />
                          <MenuItem mnemonicParsing="false" onAction="#loadMemory" text="Load Memories" />
                          <MenuItem mnemonicParsing="false" onAction="#saveMemory" text="Save Memories" />
                          <MenuItem mnemonicParsing="false" onAction="#wipeMemory" text="Wipe Memories" />
                      </items>
                  </Menu>
              </menus>
            </MenuBar>
             <ProgressBar fx:id="progressBar" layoutX="400.0" prefHeight="30.0" prefWidth="200.0" progress="0.0" />
         </children>
      </HBox>
      <TabPane fx:id="tabPane" layoutY="29.0" prefHeight="505.0" prefWidth="602.0">
         <tabs>
            <Tab closable="false" text="Main Menu">
               <content>
                  <AnchorPane layoutY="29.0" prefHeight="371.0" prefWidth="600.0" />
               </content>
            </Tab>
            <Tab closable="false" text="Interact">
               <content>
                  <AnchorPane layoutY="29.0" prefHeight="371.0" prefWidth="600.0">
                     <children>
                        <fx:include fx:id="interact" source="interact.fxml" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab closable="false" text="Examine">
               <content>
                  <AnchorPane layoutY="29.0" prefHeight="371.0" prefWidth="600.0">
                     <children>
                        <fx:include fx:id="examine" source="examine.fxml" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab closable="false" text="Parameters">
               <content>
                  <AnchorPane layoutY="29.0" prefHeight="371.0" prefWidth="600.0">
                     <children>
                        <fx:include fx:id="parameters" source="parameters.fxml" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</Pane>
